<table class="ui table ui-table">

  <thead>
    <tr>
      <th>NAO #</th>
      <th>Sales Person</th>
      <th>Revenue</th>
      <th>Prod Type</th>
    </tr>
  </thead>

  <tbody>

    {{#each model as |opp|}}
      <tr>
        <td>{{opp.id}}</td>
        <td>{{opp.apemSalesPerson}}</td>
        <td>{{opp.potentialAnnualRev}}</td>
        <td>{{opp.productType}}</td>
      </tr>
    {{/each}}

  </tbody>

</table>
